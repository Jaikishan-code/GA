                    <div class="emp-search-module">
                        <form method="post" action="/Student/Index">
                            <div class="row align-items-end">
                                <div class="col-xxl-3 col-xl-3">
                                    <label asp-for="STD_FNAME" class="form-label">First Name</label>
                                    <input asp-for="STD_FNAME" class="form-control" placeholder="First Name">
                                </div>
                                <div class="col-xxl-3 col-xl-3">
                                    <label asp-for="STD_LNAME" class="form-label">Last Name</label>
                                    <input asp-for="STD_LNAME" class="form-control" placeholder="Last Name">
                                </div>
                                <div class="col-xxl-3 col-xl-3">
                                    <label asp-for="STD_FANAME" class="form-label">Father Name</label>
                                    <input asp-for="STD_FANAME" class="form-control" placeholder="Father Name">
                                </div>
                                <div class="col-xxl-3 col-xl-3">
                                    <label asp-for="STD_MOBILENO" class="form-label">Mobile Number</label>
                                    <input asp-for="STD_MOBILENO" class="form-control" placeholder="Mobile Number">
                                </div>
                                <div class="col-xxl-3 col-xl-3">
                                    <label asp-for="STD_CLASS" class="form-label">Class</label>
                                    <div class="dropdown">
                                        <select asp-for="STD_CLASS" class="form-select" aria-label="All">
                                            <option selected>Select class</option>
                                            @foreach (var studentClass in ViewBag.Classes)
                                            {
                                                <option value="@studentClass.classid">@studentClass.stdent_Class</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="col-xxl-3 col-xl-3">
                                    <label asp-for="STD_MEDIUM" class="form-label">Medium</label>
                                    <div class="dropdown">
                                        <select asp-for="STD_MEDIUM" class="form-select" aria-label="All">
                                            <option selected>Select medium</option>
                                            <option value="Hindi">Hindi</option>
                                            <option value="English">English</option>
                                            <option value="Semi-English">Semi-English</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-xxl-3 col-xl-3">
                                    <label asp-for="STD_AFEES" class="form-label">Admission Fees</label>
                                    <input asp-for="STD_AFEES" value="100" class="form-control" readonly>
                                </div>
                                <div class="compoff-submit-btns col-xxl-2 col-xl-3">
                                    <button type="submit" class="btn apply-btn">Submit</button>
                                </div>
                            </div>
                        </form>
                    </div>

