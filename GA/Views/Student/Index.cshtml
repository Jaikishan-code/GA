                                <div class="emp-cards-module mb-5">
                                    <div class="wrapper">
                                        @foreach (var student in ViewBag.Preadmission)
                                        {
                                            <div class="emp-card">
                                                <div class="emp-img">
                                                    <img src="~/Content/assests/images/avatar.png" alt="" class="avatar">
                                                </div>
                                                <a href="edit-employee-info.html">
                                                    <img src="~/Content/assests/icons/edit-btn.svg" alt="" class="toggle">
                                                </a>
                                                <h4 class="emp-name">@($"{student.STD_FNAME} {student.STD_LNAME}")</h4>
                                                <h6 class="designation">@student.STD_CLASS</h6>
                                                <span class="badge bg-primary">Active</span>

                                                <div class="emp-info">
                                                    <div class="mail">
                                                        <span><img src="~/Content/assests/icons/mail.svg" alt="" class="mail-icon"></span>
                                                        <a href="#">@student.STD_UNIQUECODE</a>
                                                    </div>
                                                    <div class="phone">
                                                        <span><img src="~/Content/assests/icons/phone.svg" alt="" class="phone-icon"></span>
                                                        <a href="#">@student.STD_MOBILENO</a>
                                                    </div>
                                                </div>

                                                <div class="emp-role">
                                                    <div class="row role-row">
                                                        <span class="emprolh col-lg-6">Role</span>
                                                        <span class="emprolsb col-lg-6">Employee</span>
                                                    </div>
                                                    <div class="row">
                                                        <span class="emprolh col-lg-6">Date of joining</span>
                                                        <span class="emprolsb col-lg-6">@(student.STD_ENTRYDATE.ToString("dd MMM, yyyy"))</span>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>

